kinesis: "Kinesis" {
  icon: ../icons/aws/kinesis.svg
  shape: image
}

node: "App" {
  icon: ../icons/nodejs.svg
  shape: image
}

db: "Database" {
  icon: ../icons/database.png
  shape: image
}

direction: right
node -> kinesis: App sends events to kinesis
node -> db: App stores events in DB
Worker -> kinesis: Send unsent events to Kinesis
db <- Worker: pull unsent events
